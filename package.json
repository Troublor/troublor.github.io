    "@mui/material": "^6.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^19.0.2",
    "react": "^18.3.1",
    "react-dom": "^19.0.0",
    "react-ribbons": "^1.1.7",
    "react-router-dom": "^7.0.1",
    "react-scripts": "5.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "start": "REACT_APP_GIT_SHA=$(git rev-parse --short HEAD) react-scripts start",
    "build": "REACT_APP_GIT_SHA=$(git rev-parse --short HEAD) REACT_APP_BUILD_TIME=$(date +'%Y-%m-%d %H:%M:%S UTC%z') react-scripts build",
    "build:pre-release": "REACT_APP_GIT_SHA=$(git rev-parse --short HEAD) REACT_APP_BUILD_TIME=$(date +'%Y-%m-%d %H:%M:%S UTC%z') REACT_APP_PRERELEASE=true react-scripts build",
    "format": "prettier --write .",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {}
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stableVersion": "1.10.0",
  "packageManager": "yarn@4.5.0"
}
